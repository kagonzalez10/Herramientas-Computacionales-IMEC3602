    "def calculoPi(longitud, n):\n",
    "radio = float((longitud)/(math.sqrt(2-(2*((math.cos(math.radians(360/n))))))))\n",
    "diametro = 2*radio\n",
    "circunferencia = (longitud*n)\n",
    "return(float(circunferencia/diametro))"
   ]
  }
